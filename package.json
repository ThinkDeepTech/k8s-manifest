  "dependencies": {
    "@kubernetes/client-node": "0.15",
    "yaml": "^1.10.2"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^1.1.2",
    "lint-staged": "^12.4.1",
    "mocha": "^9.1.3",
    "prettier": "^2.2.1",
    "sinon": "^12.0.1"
  },
  "keywords": [
    "Kubernetes",
    "K8s",
    "Node",
    "JavaScript",
    "Client"
  ]
